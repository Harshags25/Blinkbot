generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model query {
  id                Int    @id @default(autoincrement())
  tweet_id          String
  username          String
  query             String
  response_tweet_id String
  response          String
}
